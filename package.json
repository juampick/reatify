{
  "name": "reatify",
  "license": "MIT",
  "description": "React Redux Javascript Spotify Client",
  "keywords": [
    "react",
    "redux",
    "node",
    "sass",
    "spotify"
  ],
  "author": "Juan Pablo Cook",
  "contributors": [
    {
      "name": "Juan Pablo Cook",
      "email": "juampick@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/juampick/reatify",
    "private": false
  },
  "version": "1.0.0",
  "engines": {
    "node": ">=6.9.1",
    "npm": ">=3.10.9"
  },
  "scripts": {
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "babel-watch server.js",
    "lint": "node_modules/.bin/esw webpack.config.* app tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --require ignore-styles --require mock-local-storage --reporter spec tests/testSetup.js \"app/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node tools/build.js",
    "start:dist": "babel-node tools/distServer.js"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "^6.18.0",
    "babel-watch": "^2.0.3",
    "colors": "^1.1.2",
    "compression": "^1.6.2",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-react": "^6.4.1",
    "eslint-watch": "^3.1.0",
    "expect": "^1.20.2",
    "expect-enzyme": "^1.2.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "img-loader": "^2.0.0",
    "import-glob-loader": "^1.1.0",
    "jsdom": "^9.8.3",
    "mocha": "^3.5.3",
    "mock-local-storage": "^1.0.5",
    "nock": "^9.0.0",
    "node-sass": "^4.5.1",
    "npm-run-all": "^4.0.1",
    "open": "0.0.5",
    "postcss-loader": "^1.3.3",
    "react-dom": "^15.6.1",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-logger": "^2.8.2",
    "redux-mock-store": "^1.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "body-parser": "^1.18.0",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "ignore-styles": "^5.0.1",
    "immutability-helper": "^2.2.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.8",
    "query-string": "^4.3.4",
    "randomstring": "^1.1.5",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.3",
